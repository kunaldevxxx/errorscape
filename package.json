{
  "name": "errorscape",
  "version": "1.0.0",
  "type": "module",
  "description": "A CLI tool to analyze errors and dependency issues.",
  "main": "src/cli.js",
  "bin": {
    "error-helper": "./bin/error-helper"
  },
  "scripts": {
    "start": "node src/cli.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.0",
    "ora": "^6.2.0",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!chalk)/"
    ],
    "extensionsToTreatAsEsm": [],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs"
    ]
  }
}
